# Wikipedia links with |
"(?i)\\[\\[[^\\]\u000a:]+?\\|([^\\]\u000a]+?)\\]\\]"	<_extendedContextWikiPattern>	"$0 <hasWikipediaAnchorText> @String($1)" .

# Links without |
"(?i)\\[\\[([^\\]\u000a\\|:]+?)\\]\\]"	<_extendedContextWikiPattern>	"$0 <hasWikipediaAnchorText> @String($1)" .

# Citations
"(?i)\\{\\{Cit[ae][^\\}]+title\\s*=\\s*([^\\}\\|]+)"	<_extendedContextWikiPattern>	"$0 <hasCitationTitle> @String($1)" .
"(?i)(={1,5})\\s*([^=\u000a]+?)\\s*\u0001"	<_extendedContextWikiPattern>	"$0 <hasTitleText> @String($2)" .

# External links without anchor text
"(?i)\\[https?://[^\\s\\]\u000a]+\\s+([^\\]\u000a]+)\\]"	<_extendedContextWikiPattern>	"$0 <hasWikipediaAnchorText> @String($1)" .

# Categories
"(?i)\\[\\[Category:([^\\]\u000a\\|]+).*?\\]\\]"	<_extendedContextWikiPattern>	"$0 <hasWikipediaCategory> @String($1)" .