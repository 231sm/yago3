@base <http://yago-knowledge.org/resource/> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Wikipedia links with |
"(?i)\\[\\[[^\\]\\n:]+?\\|([^\\]\\n]+?)\\]\\]"	<_extendedContextWikiPattern>	"$0 <hasWikipediaAnchorText> @String($1)" .

# Wikipedia links without |
"(?i)\\[\\[([^\\]\\n\\|:]+?)\\]\\]"	<_extendedContextWikiPattern>	"$0 <hasWikipediaAnchorText> @String($1)" .

# Citations - TODO this is not useable in non-English!
"(?i)\\{\\{[Cc]it[ae][^\\}]+title\\s*=\\s*([^\\}\\|]+)"	<_extendedContextWikiPattern>	"$0 <hasCitationTitle> @String($1)" .

# Section titles
"(?i)(={1,5})\\s*([^=\\n]+?)\\s*\\1"	<_extendedContextWikiPattern>	"$0 <hasTitleText> @String($2)" .

# External links without anchor text
"(?i)\\[https?://[^\\s\\]\\n]+\\s+([^\\]\\n]+)\\]"	<_extendedContextWikiPattern>	"$0 <hasWikipediaAnchorText> @String($1)" .