
		
# ISBN
"[ -]" mapsTo "<isbn-sep>"
			<!--<ancestor "<integer>"/> NOT YET SUPPORTED-->
		</pattern>
		<pattern name="isbn-10" regex="\d{1,5}<isbn-sep>\d{1,}<isbn-sep>\d{1,}<isbn-sep>(?:\d|X|x)\b" mapsTo 
		<pattern name="isbn-13" regex="97(8|9)<isbn-sep>\c{isbn-10}\b"/> <!-- e.g. 978-3-86680-1
"\c{isbn-10}|\c{isbn-13}" mapsTo "<isbn-num>"
"(?:ISBN|isbn)<whitespace>*<isbn-num>" mapsTo "<isbn>"
		
# URL
"\.[\p{Alpha}-]{2,}" mapsTo "<tld>"
"[\p{Alnum}-]+(?:\.[\p{Alnum}-]+)*<tld>" mapsTo "<domain>"
"\p{Alnum}+://<domain>(:?[:/][\p{Alnum}-_.~!*'();:@&amp;=+$,/?#\[\]%]*)?" mapsTo "<url>"
